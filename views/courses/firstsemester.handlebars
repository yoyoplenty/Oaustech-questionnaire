<div class="">
    {{>header}}
</div>
<div class="mt-5 pt-1">
    {{> Logo}}
</div>
<section id="banner-desktop" class="" style="padding-top: 2%;">
    <div class="container" style="height: 80vh;">
        <div class="col-md-12">
            <h5 class="text-center"> First Semester for {{student.level}} level </h5>
        </div>

        {{>messages}}

        <div class="row text-center" style="padding-top: 5vh;">

            <div class="col-6 text-center">
