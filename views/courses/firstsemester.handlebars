                <div class="my-2">
                    <h5 class="text-secondary"></h5>
                    <button class="my-btn-mobile btn-danger"><a style="text-decoration: none;"
                            class="text-light"><b>{{this.courseName}} {{this.courseCode}}</b></a></button>
                </div>
                {{/each}}

            </div>


            <div class="col-6 text-center">
                {{#each first}}
                <div class="my-2">
                    <button class="my-btn-mobile btn-danger"><a
                            href="/student/question/{{this.courseName}}{{this.courseCode}}"
                            style="text-decoration: none;" class="text-light"><b>ASSESS</b></a></button>
                </div>
                {{/each}}
            </div>



        </div>


    </div>

